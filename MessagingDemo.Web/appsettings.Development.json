{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
        //"System": "Warning"
      }
    },
    "WriteTo": [
      //{
      //  "Name": "MSSqlServer",
      //  "Args": {
      //    "connectionString": "Server=localhost;Database=Demo;Initial Catalog=Demo;Trusted_Connection=True;User ID=ap_demo;Password=ap_demo;",
      //    "tableName": "log",
      //    "schemaName": "ap_demo",
      //    "autocreateSqlTable": true
      //  }
      //},
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "log.txt",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 7,
                "rollinInterval": "day",
                "buffered": true
              }
            }
          ]
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DbConnectionString": "Server=localhost;Initial Catalog=Demo;Trusted_Connection=True;User ID=ap_demo;Password=ap_demo;"
  }
}
